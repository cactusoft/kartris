        <value>000000</value>
      </setting>
      <setting name="AgreementAPIKey" serializeAs="String">
        <value>APIkeyhere</value>
      </setting>
      <setting name="CallbackURL" serializeAs="String">
        <value>https://callbackURL</value>
      </setting>
      <setting name="ContinueURL" serializeAs="String">
        <value>https://continueURL</value>
      </setting>
      <setting name="CancelURL" serializeAs="String">
        <value>https://cancelURL</value>
      </setting>
      <setting name="PrivateKey" serializeAs="String">
