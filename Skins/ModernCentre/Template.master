				</div>

				<user:AdminBar ID="UC_AdminBar" runat="server" />


				<!-- close the off-canvas menu -->
				<a class="exit-off-canvas" onclick="closeOffCanvas()"></a>
			</div>
		</div>

		<!-- Load up javascript -->

		<script>
			$(document).foundation();
		</script>

	</form>
</body>
